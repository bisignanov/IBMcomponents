{
  "name": "@carbon/ibm-cloud-cognitive-cdai",
  "description": "Carbon for Cloud & Cognitive CD&AI UI components",
  "version": "1.25.0",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbon-design-system/ibm-products.git",
    "directory": "packages/cdai"
  },
  "bugs": "https://github.com/carbon-design-system/ibm-products/issues",
  "files": [
    "es",
    "lib",
    "scss",
    "telemetry.yml"
  ],
  "keywords": [
    "carbon",
    "carbon design system",
    "carbon community",
    "carbon for cloud & cognitive",
    "cdai"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build": "node scripts/build.js",
    "clean": "rimraf es lib scss",
    "test": "jest --transformIgnorePatterns \"node_modules/(?!@sinon)/\" --colors",
    "//upgrade-dependencies": "# don't upgrade carbon (done globally), react (explicit range peer dependency)",
    "upgrade-dependencies": "npm-check-updates -u --dep dev,peer,prod --color --reject '/(carbon|^react$|^react-dom$)/'",
    "postinstall": "ibmtelemetry --config=telemetry.yml"
  },
  "peerDependencies": {
    "@carbon/grid": "^10.43.4",
    "@carbon/icons-react": "^10.49.5",
    "carbon-components": "^10.58.15",
    "carbon-components-react": "^7.59.24",
    "prop-types": "^15.8.1",
    "react": "^16.8.6 || ^17.0.1",
    "react-dom": "^16.8.6 || ^17.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.0",
    "@ibm/telemetry-js": "^1.6.0",
    "focus-trap-react": "^10.2.3",
    "pluralize": "^8.0.0",
    "react-select": "^5.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-regenerator": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.24.7",
    "cpx": "^1.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "jest": "^29.7.0",
    "jest-config-ibm-cloud-cognitive": "^0.25.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-sass": "^8.0.0",
    "npm-check-updates": "^16.14.20",
    "rimraf": "~3 || > 4.1",
    "sinon": "^15.0.1"
  },
  "babel": {
    "presets": [
      "./scripts/env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime"
    ]
  }
}
