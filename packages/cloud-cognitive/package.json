    "clean": "rimraf es lib scss",
    "test": "bundler check 'src/**/*.scss'"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  },
  "dependencies": {
    "@carbon/ibm-cloud-cognitive-experimental": "^0.12.6",
    "@carbon/ibm-cloud-cognitive-security": "^0.2.7"
  },
  "devDependencies": {
    "@carbon/ibm-cloud-cognitive-core": "^0.4.6",
    "@carbon/rollup-config": "^0.2.2"
  }
}
